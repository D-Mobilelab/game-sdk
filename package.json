{
  "name": "docomo-game-sdk",
  "version": "2.8.0",
  "description": "Docomo digital Game SDK - JS Library",
  "main": "dist/gfsdk.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/D-Mobilelab/game-sdk"
  },
  "scripts": {
    "clean": "rm -rvf dist/*",
    "test:coverage": "jest --coverage --no-cache && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch",
    "test": "jest",
    "build:dev": "$(npm bin)/env-cmd development webpack --config tools/webpack.config.dev.js",
    "build:prod": "$(npm bin)/env-cmd production webpack --config tools/webpack.config.prod.js -p",
    "dev": "env-cmd development webpack-dev-server --config tools/webpack.config.local.js",
    "build": "npm run build:prod && npm run build:dev",
    "preversion": "npm run test",
    "version": "sh .getrevision.sh && npm run build && git add -fA src/",
    "docs": "$(npm bin)/documentation build src/SDK.js -f html --shallow -o docs/$npm_package_version",
    "append:readme": "$(npm bin)/documentation readme src/SDK.js --section=API --shallow",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "eatsjobs",
  "license": "BSD",
  "devDependencies": {
    "@storybook/addon-knobs": "^3.3.10",
    "@storybook/react": "^3.1.3",
    "autoprefixer-loader": "^3.2.0",
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "clean-webpack-plugin": "^0.1.17",
    "coveralls": "^2.13.1",
    "css-loader": "^0.25.0",
    "documentation": "^4.0.0-beta16",
    "env-cmd": "^5.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "node-http-proxy-json": "^0.1.3",
    "node-libs-browser": "^1.0.0",
    "postcss": "^5.2.6",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^1.2.1",
    "postcss-modules-values": "^1.2.2",
    "prop-types": "^15.5.10",
    "raf": "^3.4.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^3",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "blueimp-md5": "^2.10.0",
    "docomo-utils": "^1.2.8",
    "facebookpixeladapter": "^0.1.2",
    "localforage": "^1.4.3",
    "newton-adapter": "^2.6.0",
    "prop-types": "^15.5.10",
    "raven-for-redux": "^1.1.1",
    "raven-js": "^3.18.1",
    "react": "^16.5.2",
    "react-addons-css-transition-group": "^15.6.0",
    "react-ga": "^2.5.3",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.1.0"
  }
}
